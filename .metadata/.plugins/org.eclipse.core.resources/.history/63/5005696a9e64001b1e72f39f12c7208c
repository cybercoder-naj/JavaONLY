
class Outer{
	//member variable
	int a; 
	// member method
	public void show() { 
		
	}
	// member Class
	class Inner{
		
		public void display() {
			System.out.println("Hello World");
		}
		
	}
}


public class InnerDemo {

	public static void main(String[] args) {
	
		Outer obj = new Outer();
		obj.show();
		
		Outer.Inner obj1 = obj.new Inner();
		obj1.show();

	}

}
