
interface Writer{
	abstract void write();
}

class Pen implements Writer{
	public void write() {
		System.out.println("I am a Pen");
	}
}

class Pencil implements Writer{
	public void write() {
		System.out.println("I am a Pencil");
	}
}
public class InterfaceDemo {

	public static void main(String[] args) {
		Writer p = new Pen();
		Writer pc = new Pencil();
		pc.write();
		p.write();

	}

}
