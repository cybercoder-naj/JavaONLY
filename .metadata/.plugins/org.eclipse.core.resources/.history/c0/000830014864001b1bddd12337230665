class Calc{
	
	int num1;
	int num2;
	
	public int add() {
		return num1+num2;
	}
	public int sub() {
		return num1+num2;
	}
	public int mul() {
		return num1+num2;
	}
	public int divide() {
		return num1+num2;
	}
	
}
public class ClassObjects {
	public static void main(String args[]) {
		Calc obj = new Calc();
		obj.num1 = 5;
		obj.num2 = 8;
		System.out.println(obj.add());
		
	}

}
