class Calc{
	
	int num1;
	int num2;
	
	public Calc() {
		num1 = 5;
		num2 = 6;
	}
	
	public Calc(int n) {
		num1 = n;
	}
	
	public int add() {
		return num1+num2;
	}
	public int sub() {
		return num1-num2;
	}
	public int mul() {
		return num1*num2;
	}
	public int divide() {
		return num1/num2;
	}
	
}
public class ClassObjects {
	public static void main(String args[]) {
		Calc obj = new Calc(7);
		System.out.println(obj.add());
		System.out.println(obj.sub());
		System.out.println(obj.mul());
		System.out.println(obj.divide());
		
	}

}
